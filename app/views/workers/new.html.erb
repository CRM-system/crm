<h1>Add workers</h1>

<%= form_for :worker, url: workers_path, method: :post do |f| %>


<p>
	<%= f.label :nickname %><br/>
	<%= f.text_field :nickname %>
</p>

<p>
	<%= f.label :email %><br/>
	<%= f.text_field :email %>
</p>

<p>
	<%= f.label :password %><br/>
	<%= f.text_field :password %>
</p>

<p>
	<%= f.submit %>
</p>
<% end %>