<% if @products %>
	<% @products.each do |product| %>
		<%= link_to product.name, admin_product_path(product)%>
		<td><%= link_to 'Удалить', admin_product_path(product), method: :delete, data: { confirm: 'Are you sure?' } %></td><br>
	<% end %>
<% end %>
<%= link_to 'Создать новый товар',   new_admin_product_path %>